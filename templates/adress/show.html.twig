{% extends 'base.html.twig' %}

{% block title %}Adress{% endblock %}

{% block body %}
    <h1>Adress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adress.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ adress.name }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ adress.numero }}</td>
            </tr>
            <tr>
                <th>Codepostal</th>
                <td>{{ adress.codepostal }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ adress.ville }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('account_adresses') }}">Retourner au adresses</a>

    

    {{ include('adress/_delete_form.html.twig') }}
{% endblock %}
